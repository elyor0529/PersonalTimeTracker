<Window x:Class="TimeTracker.ShareDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimeTracker"
        mc:Ignorable="d"
        Title="ShareDialog" Height="100" Width="300">
    <Grid>
        <StackPanel>
        <TextBlock>Share task with</TextBlock>
        <TextBox Name="Email"></TextBox>
        <Button Name="shareBtn" Click="shareBtn_Click">share</Button>
            <TextBlock Name="resultTextBlock"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
